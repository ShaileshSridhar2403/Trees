{
  "name": "trees-ui",
  "homepage": "./",
  "main": "./electron/main.js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "d3": "^6.3.1",
    "d3-flextree": "^2.1.1",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.1",
    "react": "^16.9.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.9.0",
